{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "required": [
    "type"
  ],
  "properties": {
    "__type__": {
      "const": "layer"
    },
    "__comments__": {
      "type": "object"
    },
    "__position__": {
      "type": "object"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "classes": {
      "type": "array",
      "items": {
        "$ref": "class.json"
      }
    },
    "classgroup": {
      "type": "string"
    },
    "classitem": {
      "type": "string"
    },
    "cluster": {
      "allOf": [
        {
          "$ref": "cluster.json"
        }
      ],
      "metadata": {
        "minVersion": 6
      }
    },
    "composites": {
      "type": "array",
      "items": {
        "$ref": "composite.json"
      },
      "metadata": {
        "minVersion": 7
      }
    },
    "connection": {
      "type": "string"
    },
    "connectionoptions": {
      "allOf": [
        {
          "$ref": "connectionoptions.json"
        }
      ],
      "metadata": {
        "minVersion": 7.6
      }
    },
    "connectiontype": {
      "enum": [
        "contour",
        "kerneldensity",
        "idw",
        "local",
        "ogr",
        "oraclespatial",
        "plugin",
        "postgis",
        "sde",
        "union",
        "uvraster",
        "wfs",
        "wms",
        "mygis",
        "flatgeobuf"
      ]
    },
    "data": {
      "type": "string"
    },
    "debug": {
      "allOf": [
        {
          "$ref": "debug.json"
        }
      ],
      "metadata": {
        "minVersion": 5
      }
    },
    "dump": {
      "oneOf": [
        {
          "type": "boolean"
        },
        {
          "$ref": "onoff.json"
        }
      ],
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "encoding": {
      "type": "string",
      "metadata": {
        "minVersion": 7
      }
    },
    "extent": {
      "$ref": "extent.json"
    },
    "features": {
      "type": "array",
      "items": {
        "$ref": "feature.json"
      }
    },
    "filter": {
      "$ref": "expression.json"
    },
    "filteritem": {
      "type": "string"
    },
    "footer": {
      "type": "string"
    },
    "geomtransform": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "string",
          "pattern": "^\\((.*?)\\)$"
        }
      ],
      "metadata": {
        "minVersion": 6.4
      }
    },
    "grid": {
      "$ref": "grid.json"
    },
    "group": {
      "type": "string"
    },
    "header": {
      "type": "string"
    },
    "joins": {
      "type": "array",
      "items": {
        "$ref": "join.json"
      }
    },
    "labelangleitem": {
      "type": "string",
      "metadata": {
        "maxVersion": 5.6
      }
    },
    "labelcache": {
      "$ref": "onoff.json"
    },
    "labelitem": {
      "type": "string"
    },
    "labelmaxscale": {
      "type": "number",
      "metadata": {
        "maxVersion": 5
      }
    },
    "labelminscale": {
      "type": "number",
      "metadata": {
        "maxVersion": 5
      }
    },
    "labelmaxscaledenom": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 5
      }
    },
    "labelminscaledenom": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 5
      }
    },
    "labelrequires": {
      "type": "string"
    },
    "labelsizeitem": {
      "type": "string",
      "metadata": {
        "maxVersion": 5
      }
    },
    "mask": {
      "type": "string",
      "metadata": {
        "minVersion": 6.2
      }
    },
    "maxfeatures": {
      "type": "integer"
    },
    "maxgeowidth": {
      "type": "number",
      "metadata": {
        "minVersion": 5.4
      }
    },
    "maxscaledenom": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 5
      }
    },
    "maxscale": {
      "type": "number",
      "metadata": {
        "maxVersion": 5
      }
    },
    "metadata": {
      "$ref": "metadata.json"
    },
    "minfeaturesize": {
      "type": "number"
    },
    "mingeowidth": {
      "type": "number",
      "metadata": {
        "minVersion": 5.4
      }
    },
    "minscaledenom": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 5
      }
    },
    "minscale": {
      "type": "number",
      "metadata": {
        "maxVersion": 5
      }
    },
    "name": {
      "type": "string"
    },
    "offsite": {
      "$ref": "color.json"
    },
    "opacity": {
      "type": "integer",
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "plugin": {
      "type": "string"
    },
    "postlabelcache": {
      "type": "boolean"
    },
    "processing": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "projection": {
      "$ref": "projection.json"
    },
    "requires": {
      "type": "string"
    },
    "scaletokens": {
      "type": "array",
      "items": {
        "$ref": "scaletoken.json"
      },
      "metadata": {
        "minVersion": 6.4
      }
    },
    "sizeunits": {
      "enum": [
        "feet",
        "inches",
        "kilometers",
        "meters",
        "miles",
        "nauticalmiles",
        "pixels"
      ]
    },
    "status": {
      "default": "off",
      "type": "string",
      "enum": [
        "on",
        "off",
        "default"
      ]
    },
    "styleitem": {
      "type": "string"
    },
    "symbolscaledenom": {
      "type": "number",
      "minimum": 1,
      "metadata": {
        "minVersion": 5
      }
    },
    "template": {
      "type": "string"
    },
    "tileindex": {
      "type": "string"
    },
    "tileitem": {
      "default": "location",
      "type": "string"
    },
    "tilesrs": {
      "type": "string"
    },
    "tolerance": {
      "type": "number"
    },
    "toleranceunits": {
      "enum": [
        "pixels",
        "feet",
        "inches",
        "kilometers",
        "meters",
        "miles",
        "nauticalmiles",
        "dd"
      ]
    },
    "transparency": {
      "oneOf": [
        {
          "type": "integer"
        },
        {
          "const": "alpha"
        }
      ],
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "transform": {
      "oneOf": [
        {
          "type": "boolean"
        },
        {
          "$ref": "position.json"
        }
      ]
    },
    "type": {
      "enum": [
        "chart",
        "circle",
        "line",
        "point",
        "polygon",
        "raster",
        "query",
        "annotation",
        "tileindex"
      ]
    },
    "units": {
      "default": "meters",
      "enum": [
        "dd",
        "feet",
        "inches",
        "kilometers",
        "meters",
        "miles",
        "nauticalmiles",
        "percentages",
        "pixels"
      ]
    },
    "utfdata": {
      "type": "string",
      "metadata": {
        "minVersion": 7
      }
    },
    "utfitem": {
      "type": "string",
      "metadata": {
        "minVersion": 7
      }
    },
    "validation": {
      "$ref": "validation.json"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}