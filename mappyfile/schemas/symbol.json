{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "symbol"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "anchorpoint": {
        "type": "array",
        "items": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "minItems": 2,
        "maxItems": 2,
        "default": [ 0.5, 0.5 ],
        "metadata": {
            "minVersion": 6.2
        }
    },
    "antialias": {
      "type": "boolean",
      "default": false,
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "backgroundcolor": {
      "$ref": "color.json"
    },
    "character": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 1
        },
        {
          "pattern": "^&#[0-9]+;$",
          "example": "&#10140;",
          "type": "string"
        }
      ]
    },
    "filled": {
      "type": "boolean",
      "default": false
    },
    "font": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "points": {
      "$ref": "points.json"
    },
    "transparent": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255,
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "type": {
      "enum": [
        "ellipse",
        "hatch",
        "pixmap",
        "svg",
        "truetype",
        "vector"
      ]
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}