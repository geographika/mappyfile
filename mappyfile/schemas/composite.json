{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "composite"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "opacity": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "compfilter": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "compop": {
      "type": "string",
      "enum": [
        "clear",
        "color-burn",
        "color-dodge",
        "contrast",
        "darken",
        "difference",
        "dst",
        "dst-atop",
        "dst-in",
        "dst-out",
        "dst-over",
        "exclusion",
        "hard-light",
        "invert",
        "invert-rgb",
        "lighten",
        "minus",
        "multiply",
        "overlay",
        "plus",
        "screen",
        "soft-light",
        "src",
        "src-atop",
        "src-in",
        "src-out",
        "src-over",
        "xor"
      ]
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}