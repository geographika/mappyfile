{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "grid"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "labelformat": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "enum": [
            "dd",
            "ddmm",
            "ddmmss"
          ]
        }
      ]
    },
    "minarcs": {
      "type": "number",
      "default": 16
    },
    "maxarcs": {
      "type": "number"
    },
    "mininterval": {
      "type": "number"
    },
    "maxinterval": {
      "type": "number"
    },
    "minsubdivide": {
      "type": "number"
    },
    "maxsubdivide": {
      "type": "number",
      "default": 256
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}