{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": {}
  },
  "properties": {
    "__type__": {
      "enum": [ "grid" ]
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "labelformat": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "enum": [ "dd", "ddmm", "ddmmss" ]
        }
      ]
    },
    "minarcs": {
      "type": "number",
      "default": 16,
      "exclusiveMinimum": 0
    },
    "maxarcs": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "mininterval": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "maxinterval": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "minsubdivide": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "maxsubdivide": {
      "type": "number",
      "default": 256,
      "exclusiveMinimum": 0
    }
  }
}
