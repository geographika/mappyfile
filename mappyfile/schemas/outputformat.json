{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "outputformat"
    },
    "__comments__": {
      "type": "object"
    },
    "__position__": {
      "type": "object"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "driver": {
      "type": "string"
    },
    "extension": {
      "type": "string"
    },
    "formatoption": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "imagemode": {
      "type": "string",
      "enum": [
        "pc256",
        "rgb",
        "rgba",
        "byte",
        "int16",
        "float32",
        "feature"
      ],
      "additionalProperties": false
    },
    "mimetype": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "transparent": {
      "oneOf": [
        {
          "type": "boolean"
        },
        {
          "$ref": "onoff.json"
        }
      ]
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}