{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "reference"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "color": {
      "$ref": "color.json"
    },
    "extent": {
      "$ref": "extent.json"
    },
    "image": {
      "type": "string",
      "description": "filename"
    },
    "marker": {
      "oneOf": [
        {
          "type": "integer",
          "minimum": 0
        },
        {
          "type": "string"
        }
      ]
    },
    "markersize": {
      "type": "integer"
    },
    "minboxsize": {
      "type": "integer"
    },
    "maxboxsize": {
      "type": "integer"
    },
    "outlinecolor": {
      "$ref": "color.json"
    },
    "size": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 5
      },
      "minItems": 2,
      "maxItems": 2
    },
    "status": {
      "$ref": "onoff.json"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}