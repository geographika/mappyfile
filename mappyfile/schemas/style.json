{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "style"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "angle": {
      "oneOf": [
        {
          "type": "number"
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute"
        },
        {
          "const": "auto"
        }
      ]
    },
    "angleitem": {
      "type": "string",
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "antialias": {
      "type": "boolean",
      "default": false,
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "backgroundcolor": {
      "allOf": [
        {
          "$ref": "color.json"
        }
      ],
      "metadata": {
        "maxVersion": 7.6
      }
    },
    "color": {
      "oneOf": [
        {
          "$ref": "color.json"
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute",
          "metadata": {
            "minVersion": 5
          }
        }
      ]
    },
    "colorrange": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "type": "integer"
          },
          "minItems": 6,
          "maxItems": 6
        },
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 2,
          "maxItems": 2
        }
      ]
    },
    "datarange": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 2,
      "maxItems": 2
    },
    "gap": {
      "type": "number",
      "metadata": {
        "minVersion": 6
      }
    },
    "geomtransform": {
      "oneOf": [
        {
          "enum": [
            "bbox",
            "centroid",
            "end",
            "labelpnt",
            "labelpoly",
            "labelcenter",
            "start",
            "vertices"
          ]
        },
        {
          "type": "string",
          "pattern": "^\\((.*?)\\)$",
          "description": "expression"
        }
      ],
      "metadata": {
        "minVersion": 5.4
      }
    },
    "initialgap": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 6.2
      }
    },
    "linecap": {
      "enum": [
        "butt",
        "round",
        "square"
      ],
      "metadata": {
        "minVersion": 6
      }
    },
    "linejoin": {
      "enum": [
        "round",
        "miter",
        "bevel",
        "none"
      ],
      "metadata": {
        "minVersion": 6
      }
    },
    "linejoinmaxsize": {
      "type": "integer",
      "default": 3,
      "metadata": {
        "minVersion": 6
      }
    },
    "maxscaledenom": {
      "type": "number",
      "minimum": 0,
      "metadata": {
        "minVersion": 5
      }
    },
    "maxsize": {
      "type": "number",
      "default": 500
    },
    "maxwidth": {
      "type": "number",
      "default": 32
    },
    "minscaledenom": {
      "type": "number",
      "minimum": 0
    },
    "minsize": {
      "type": "number",
      "default": 0,
      "minimum": 0
    },
    "minwidth": {
      "type": "number",
      "default": 0,
      "minimum": 0
    },
    "offset": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "number"
          },
          {
            "type": "string",
            "pattern": "^\\[(.*?)\\]$",
            "description": "attribute"
          }
        ],
        "minItems": 2,
        "maxItems": 2
      }
    },
    "opacity": {
      "oneOf": [
        {
          "type": "integer"
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute",
          "metadata": {
            "minVersion": 5.6
          }
        }
      ]
    },
    "outlinecolor": {
      "oneOf": [
        {
          "$ref": "color.json"
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute",
          "metadata": {
            "minVersion": 5
          }
        }
      ]
    },
    "outlinewidth": {
      "oneOf": [
        {
          "type": "number",
          "default": 0,
          "minimum": 0
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute"
        }
      ],
      "metadata": {
        "minVersion": 5.4
      }
    },
    "pattern": {
      "allOf": [
        {
          "$ref": "points.json"
        }
      ],
      "metadata": {
        "minVersion": 6
      }
    },
    "polaroffset": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "number"
          },
          {
            "type": "string",
            "pattern": "^\\[(.*?)\\]$",
            "description": "attribute"
          }
        ],
        "minItems": 2,
        "maxItems": 2
      },
      "metadata": {
        "minVersion": 6.2
      }
    },
    "rangeitem": {
      "type": "string"
    },
    "size": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute",
          "metadata": {
            "minVersion": 5
          }
        },
        {
          "allOf": [
            {
              "$ref": "expression.json"
            }
          ],
          "metadata": {
            "minVersion": 8.2
          }
        }
      ]
    },
    "symbol": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "number",
          "minimum": 0
        },
        {
          "$ref": "symbol.json"
        }
      ]
    },
    "width": {
      "oneOf": [
        {
          "type": "number",
          "default": 1,
          "minimum": 0
        },
        {
          "type": "string",
          "pattern": "^\\[(.*?)\\]$",
          "description": "attribute",
          "metadata": {
            "minVersion": 5.4
          }
        }
      ]
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}