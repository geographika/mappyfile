{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^__[a-z]+__$": true
  },
  "properties": {
    "__type__": {
      "const": "cluster"
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "maxdistance": {
      "type": "number",
      "default": 10
    },
    "region": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^rectangle$"
        },
        {
          "type": "string",
          "pattern": "^ellipse$"
        }
      ]
    },
    "buffer": {
      "type": "number",
      "default": 0
    },
    "group": {
      "$ref": "expression.json"
    },
    "filter": {
      "$ref": "expression.json"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}